{
  "name": "miit",
  "version": "1.1.0",
  "description": "Miit",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/yuanzhhh/miit"
  },
  "keywords": [
    "miit",
    "mitt",
    "event-emitter",
    "emitter"
  ],
  "author": "yuanzhhh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yuanzhhh/miit/issues"
  },
  "homepage": "https://github.com/yuanzhhh/miit",
  "files": [
    "dist",
    "src",
    "types",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rimraf dist types coverage",
    "dev": "pnpm install && cross-env NODE_ENV=development rollup -c -w",
    "cp-package": "cp ./package.json ./dist",
    "runAfterBuild": "node ./afterBuild.js",
    "build": "rm -rf stats.html && pnpm install && pnpm format && pnpm lint && pnpm test && pnpm coverage && pnpm clean && cross-env NODE_ENV=production rollup -c && npm-run-all -s cp-package runAfterBuild",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.js",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.3",
    "@size-limit/preset-small-lib": "^11.2.0",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.25.5",
    "eslint": "^9.29.0",
    "husky": "9.1.7",
    "jest": "^30.0.3",
    "lint-staged": "^16.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "3.6.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.44.0",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-esbuild": "^6.2.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "size-limit": "^11.2.0",
    "ts-jest": "^29.4.0",
    "typedoc": "^0.28.5",
    "typescript": "~5.8.3"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "rxjs": "7.8.2"
  }
}
