{
  "name": "rollup-boilerplate",
  "version": "0.0.1",
  "description": "Rollup boilerplate",
  "main": "dist/rollup-boilerplate.cjs.js",
  "module": "dist/rollup-boilerplate.es.js",
  "types": "types/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "files": [
    "dist",
    "src",
    "types",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rimraf dist types coverage",
    "dev": "pnpm install && cross-env NODE_ENV=development rollup -c -w",
    "obfuscator-dist": "javascript-obfuscator ./dist --output ./dist --config ./obfuscator.config.json",
    "cp-package": "cp ./package.json ./dist",
    "runAfterBuild": "node ./afterBuild.js",
    "build": "pnpm install && pnpm clean && cross-env NODE_ENV=production rollup -c && npm-run-all -s obfuscator-dist cp-package runAfterBuild",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.js",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@size-limit/preset-small-lib": "^11.0.1",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.19.10",
    "eslint": "^8.0.0",
    "husky": "9.1.7",
    "javascript-obfuscator": "4.1.1",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.1",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-esbuild": "^6.1.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "size-limit": "^11.0.1",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.25.4",
    "typescript": "~5.3.3"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
